{
  "name": "gatsby-starter-typescript-sass",
  "description": "Gatsby typescript sass starter",
  "title": "My Package Title",
  "license": "MIT",
  "version": "1.1.3",
  "scripts": {
    "start": "gatsby develop",
    "dev": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve"
  },
  "dependencies": {
    "gatsby": "^2.15.16",
    "gatsby-plugin-facebook-sdk": "^1.0.6",
    "gatsby-plugin-sass": "^4.14.0",
    "gatsby-theme-auth0": "^1.1.1",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "node-sass": "^4.13.1",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.3.0",
    "sass": "^1.42.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.21.0",
    "webpack": "^4.36.1"
  },
  "devDependencies": {
    "@types/react-helmet": "^6.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "pretty-quick",
      "eslint --fix",
      "stylelint . --fix",
      "git add"
    ],
    "*.{json,html}": [
      "pretty-quick",
      "git add"
    ],
    "*.{css,scss}": [
      "stylelint . --fix",
      "pretty-quick",
      "git add"
    ]
  }
}
